### ğŸ¬ Animation Type Quick Reference
```
â”Œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”
â”‚  ANIMATION TYPE        â”‚  FORMULA                              â”‚  BEST FOR                   â”‚
â”œâ”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”¤
â”‚  QUANTUM_WAVE          â”‚  sin((time*freq) + (pos*wavelength))   â”‚  Headers, titles            â”‚
â”‚  NEURAL_PULSE          â”‚  exp(-distÂ²/2ÏƒÂ²) * sin(time*freq)     â”‚  Status indicators         â”‚
â”‚  HOLOGRAPHIC_GLOW      â”‚  Î£(sin(time*freq_i + phase_i))/n      â”‚  Important elements        â”‚
â”‚  DATA_STREAM           â”‚  Time-based probability                â”‚  Metrics, data              â”‚
â”‚  WAVE                  â”‚  sin((x*wavelength) + (time*freq))   â”‚  Backgrounds                â”‚
â”‚  PULSE                 â”‚  0.5 + 0.5*sin(time*freq)             â”‚  Active states             â”‚
â”‚  SPARKLE               â”‚  Random with probability               â”‚  Highlights, events        â”‚
â”‚  GLOW                  â”‚  base*(1 + intensity*sin(time))       â”‚  Text, borders             â”‚
â”‚  RAINBOW               â”‚  HSV(time*speed % 360, 1.0, 1.0)       â”‚  Special effects           â”‚
â”‚  SCAN                  â”‚  (time*speed) % height                 â”‚  Loading, progress        â”‚
â”‚  FADE                  â”‚  sin(time*freq)                        â”‚  Transitions               â”‚
â”‚  BLINK                 â”‚  (time % period) < (period*duty)      â”‚  Alerts, warnings         â”‚
â”‚  BOUNCE                â”‚  amplitude*abs(sin(time*freq))         â”‚  Interactive elements     â”‚
â”‚  FLOW                  â”‚  (time*speed) % length                  â”‚  Continuous motion        â”‚
â””â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”˜
